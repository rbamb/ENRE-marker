{
  "name": "enre-marker",
  "displayName": "ENRE-marker",
  "description": "ENRE-marker is a tool for manually label truth entities and relations analysed by well-known tools.",
  "version": "0.0.1",
  "publisher": "ENRE",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:enre-marker.management"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "enre-marker.management",
        "title": "Management Center",
        "category": "ENRE-marker"
      },
      {
        "command": "enre-marker.test",
        "title": "Show color in editor view",
        "category": "ENRE-marker"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p -w . --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/react": "^17.0.31",
    "@types/react-dom": "^17.0.10",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.4.0",
    "eslint": "^7.32.0",
    "file-loader": "^6.2.0",
    "glob": "^7.1.7",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.4.3",
    "mocha": "^9.1.1",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "antd": "^4.16.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
  }
}
